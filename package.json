{
  "name": "zezinho",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --base-href https://lcpereira.github.io/zezinho/ --deploy-url https://lcpereira.github.io/zezinho/",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage --no-cache"
  },
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,html}": "prettier --write"
  },
  "dependencies": {
    "@angular/animations": "^12.2.7",
    "@angular/common": "^12.2.7",
    "@angular/compiler": "^12.2.7",
    "@angular/core": "^12.2.7",
    "@angular/forms": "^12.2.7",
    "@angular/google-maps": "^12.2.7",
    "@angular/platform-browser": "^12.2.7",
    "@angular/platform-browser-dynamic": "^12.2.7",
    "@angular/router": "^12.2.7",
    "@apollo/client": "^3.4.15",
    "@fontsource/permanent-marker": "^4.5.0",
    "@fontsource/roboto": "^4.5.1",
    "@mdi/font": "^5.9.55",
    "apollo-angular": "^2.6.0",
    "graphql": "^15.6.0",
    "ngx-skeleton-loader": "^4.0.0",
    "ngx-toastr": "^14.1.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.7",
    "@angular/cli": "^12.2.7",
    "@angular/compiler-cli": "^12.2.7",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-angular": "^13.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.20.27",
    "husky": "^4.3.8",
    "jest": "^27.2.3",
    "jest-preset-angular": "^9.0.7",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.3.5"
  }
}
